Private Sub xcRFC_Chk(ChkStrg As String, retval As Integer)
' ID......: Check al capturar el RFC del cliente
' Concepto: FunciÛn que valida si existe ya un RFC dado de alta dentro de la tabla de clientes
' BIT¡CORA DE CAMBIOS
' ID        FECHA       USUARIO         MODIFICACI”N
' 001       12/05/2013  Rodolfo GarcÌa  Al capturar el RFC validar· dentro de la pantalla de clientes adicional xFESoaddress que no exista otro cliente con ese RFC.
'                                       En caso de existir mandar· una alerta con el Cliente Id y nombre

'---- INICIO 001
    If Trim(ChkStrg) <> "XAXX010101000" Then
        Dim sSql As String
        sSql = "Select * from xfesoaddress where rfc = " + SParm(ChkStrg)
        serr1_xfesoaddres = SqlFetch1(csr1_xfesoaddres, sSql, bxfesoaddress1, LenB(bxfesoaddress1))
        If serr1_xfesoaddres = 0 Then
            MessBox "El RFC que est·s dando de alta ya le pertenece al cliente " + SParm(Trim(bxfesoaddress.custid) + " - " + Trim(bxfesoaddress.nombre)), MB_ICONINFORMATION, "RFC duplicado"
        End If
        sivMyApp.Controls("ctaxlocid").value = ChkStrg
    End If
'---- FIN 001
End Sub

Private Sub xcTipoPago_Chk(ChkStrg As String, retval As Integer)
    If Trim(ChkStrg) = "F" Then
        sivMyApp.Controls("xcRazonSocial").Properties("Enabled") = False
        sivMyApp.Controls("xcApellidoMaterno").Properties("Enabled") = True
        sivMyApp.Controls("xcApellidoPaterno").Properties("Enabled") = True
        sivMyApp.Controls("xcNombre").Properties("Enabled") = True
    ElseIf Trim(ChkStrg) = "M" Or Trim(ChkStrg) = "E" Then
        sivMyApp.Controls("xcRazonSocial").Properties("Enabled") = True
        sivMyApp.Controls("xcApellidoMaterno").Properties("Enabled") = False
        sivMyApp.Controls("xcApellidoPaterno").Properties("Enabled") = False
        sivMyApp.Controls("xcNombre").Properties("Enabled") = False
    End If
End Sub

Private Sub Update1_OnUpdate(ByVal Level As Integer, ByVal InsertFlg As Integer, retval As Integer)
' ID......: Fin de evento al actualizar
' Concepto: Evento al actualizar (Actualiza registro o finalizar registro ya existente)
' BIT¡CORA DE CAMBIOS
' ID        FECHA       USUARIO         MODIFICACI”N
' 001       09/07/2007  Rodolfo GarcÌa  Al insertar registros en xFESOAddress (Tabla Adicional de Clientes). Se agregÛ que validara el nivel para que se ejecutar· solo en el nivel cero y valida la variable global. Si si existe entonces manda a llamar funciÛn de UPDATE
'                                       En caso contrario manda a llamar a INSERT y actualiza la variable para que cualquier cambio sea UPDATE no INSERT.

'INICIO 001
'----------------------
    If Level = 0 Then 'Valida el nivel que sea cero (primero)
        If sExistexCustomerAdic = "S" Then 'Si existe ya registro en la tabla adicional entonces solamente se actualizar·
            Call SUpdate1(CSR_xFESOAddress, "xFESOAddress", bxfesoaddress, LenB(bxfesoaddress)) 'FunciÛn para actualizar registro del cliente
        Else 'Si no existe registro en la tabla adicional entonces manda a INSERT
            Call SInsert1(CSR_xFESOAddress, "xFESOAddress", bxfesoaddress, LenB(bxfesoaddress)) 'FunciÛn de INSERT de los registros
            sExistexCustomerAdic = "S" 'Actualiza variable Global como ya insertado por lo que nuevos cambios son ACTUALIZADOS
        End If
    End If
'----------------------
'FIN '001
End Sub

