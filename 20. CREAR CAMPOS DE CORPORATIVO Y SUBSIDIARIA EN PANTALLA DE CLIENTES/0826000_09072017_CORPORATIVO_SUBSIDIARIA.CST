Option Explicit
Dim sExistexCustomerAdic As String * 1

Private Sub cclassid_Chk(ChkStrg As String, retval As Integer)
' ID......: Check al capturar el cliente id
' Concepto: FunciÛn que determina si los campos de Tipo de Cliente, Cond de Pago e Impuesto se habilitan y sus valores default
' BIT¡CORA DE CAMBIOS
' ID        FECHA       USUARIO         MODIFICACI”N
' 001       01/09/2013  Rodolfo GarcÌa  Al capturar el tipo de cliente verifica, si viene de un cliente nuevo pone los valores default del tipo de cliente CONTADO,
'                                       en caso contrario busca el valor capturado para poner los valores correspondientes.

'---- INICIO 001
    Dim sSql As String
    If serr_custclass = 0 Then
        sivMyApp.Controls("cTerms").Properties("Enabled") = True
        sivMyApp.Controls("cTerms").value = bCustClass.Terms
        sivMyApp.Controls("cTerms").Properties("Enabled") = False

        sivMyApp.Controls("ctaxid1").Properties("Enabled") = True
        sivMyApp.Controls("ctaxid1").value = bCustClass.User6
        sivMyApp.Controls("ctaxid1").Properties("Enabled") = False
    ElseIf serr_custclass = 123 Then
        sSql = "Select * From CustClass where classid = " + SParm(ChkStrg)
        serr_custclass = SqlFetch1(csr_custclass, sSql, bCustClass, LenB(bCustClass))
        If serr_custclass = 0 Then
            sivMyApp.Controls("cTerms").value = bCustClass.Terms
            sivMyApp.Controls("ctaxid1").value = bCustClass.User6
            End If
        serr_custclass = 123
    End If
'---- FIN 001
End Sub
