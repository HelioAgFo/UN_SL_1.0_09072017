Private Sub Update1_OnUpdate(ByVal Level As Integer, ByVal InsertFlg As Integer, retval As Integer)
    ' ID......: Fin de evento al actualizar
' Concepto: Evento al actualizar (Actualiza registro o finalizar registro ya existente)
' BITÁCORA DE CAMBIOS
' ID        FECHA       USUARIO         MODIFICACIÓN
' 001       07/09/2013  Rodolfo García  Al guardar verifica en base a la tabla siuserappauth (Valores Omisión Usuario) si el usuario validará
'                                       que el embarque debe estar totalmente pagado para clientes contado o no pagado para clientes crédito

'---- INICIO 001
    Dim sUsuario As String
    Dim sSql As String
    Dim bValidaEmbarqueCreditoContado As Boolean
    Dim sTipoOrden As String

    Call GetBufferValue("bSoType.Behavior", sTipoOrden)
    If Trim(sTipoOrden) = "CS" Then
        Call GetBufferValue("bpes.userid", sUsuario)
        bValidaEmbarqueCreditoContado = False
        sSql = "Select * From SIUserAppAuth Where UserId = " + SParm(sUsuario)

        serr_SIUserAppAuth = SqlFetch1(csr_SIUserAppAuth, sSql, bSIUserAppAuth, LenB(bSIUserAppAuth))
        If serr_SIUserAppAuth = 0 Then
            If Trim(bSIUserAppAuth.User5) = "1" Then
                bValidaEmbarqueCreditoContado = fnValidaEmbarqueCreditoContado
            End If
        Else
            bValidaEmbarqueCreditoContado = fnValidaEmbarqueCreditoContado
        End If
        If bValidaEmbarqueCreditoContado Then
            retval = ErrNoMess
        End If
    End If
'---- FIN 001

    Dim sMetodoPago As String
    Dim sPmtTypeId As String
    Dim sNumeroCuenta As String
    Dim sNumeroCuentaHeader As String
    Dim sUserID As String
    Dim sShipperId As String
    Dim sCustId As String

    If Level = 32001 Then
        sCustId = Trim(sivMyApp.Controls("ccustid_0").value)
        Call GetBufferValue("bSOShipheader.ShipperID", sShipperId)
        Call GetBufferValue("bpes.UserID", sUserID)
        Call GetBufferValue("bpes.cpnyid", sCpnyId)
        sCpnyId = Trim(sCpnyId)
        sPmtTypeId = sivMyApp.Controls("cpmttypeid_5").value
        sSql = "Select * From PmtType Where PmtTypeId = " + SParm(sPmtTypeId)
        serr_PmtType = SqlFetch1(csr_PmtType, sSql, bPmtType, LenB(bPmtType))
        If serr_PmtType = 0 Then
            Select Case Trim(bPmtType.User7)
                Case "CHQ"
                     sMetodoPago = "CHEQUE NOMINATIVO"
                Case "TRA"
                    sMetodoPago = "TRANSFERENCIA ELECTRÓNICA"
                Case "EFE"
                    sMetodoPago = "EFECTIVO"
                Case "TCR"
                    sMetodoPago = "TARJETA"
                Case Else
                    sMetodoPago = "NO IDENTIFICADO"
            End Select
            sivMyApp.Controls("xcMetodoDePago").Properties("Enabled") = True
            sivMyApp.Controls("xcNumeroDeCuenta").Properties("Enabled") = True
            sivMyApp.Controls("xcMetodoDePago").value = sMetodoPago
            sNumeroCuenta = sivMyApp.Controls("cchknbr_5").value
            sivMyApp.Controls("xcNumeroDeCuenta").value = Trim(sNumeroCuenta)
            sivMyApp.Controls("xcNumeroDeCuenta").Properties("Enabled") = False
            sivMyApp.Controls("xcMetodoDePago").Properties("Enabled") = False
        End If

        Call TranBeg(True)
            Call sql(c1, "xsp_xSoShipHeaderAdicInsert" & SParm(Trim(sCpnyId)) & SParm(sShipperId) & SParm(bxSoShipHeaderAdic.MetodoDePago) & SParm(bxSoShipHeaderAdic.NumeroDeCuenta) & SParm(bxSoShipHeaderAdic.User6))
            Call SqlFree(c1)
        Call TranEnd
    End If
End Sub

