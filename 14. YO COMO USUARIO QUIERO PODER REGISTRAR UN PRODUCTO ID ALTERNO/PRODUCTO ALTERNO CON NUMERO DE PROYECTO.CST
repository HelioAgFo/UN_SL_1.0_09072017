Public Function fnValidaEmbarqueCreditoContado() As Boolean
' ID......: Función para validar embarque crédito contado
' Concepto: Función que valida la verificación del embarque, si es contado y tiene saldo TRUE, si es crédito y tiene pagos TRUE, todo lo demás TRUE
' BITÁCORA DE CAMBIOS
' ID        FECHA       USUARIO         MODIFICACIÓN
' 001       07/09/2013  Rodolfo García  Al guardar verifica en base a la tabla siuserappauth (Valores Omisión Usuario) si el usuario validará
'                                       que el embarque debe estar totalmente pagado para clientes contado o no pagado para clientes crédito

'---- INICIO 001
    Dim Sqlst As String
    Dim sCustId As String
    Dim dSaldo As Double
    Dim dMontoFactura As Double
    Dim dPagos As Double
    
    sCustId = sivMyApp.Controls("ccustid_0").value
    dSaldo = sivMyApp.Controls("cCuryBalDue_0").value
    dPagos = sivMyApp.Controls("ccurytotpmt_0").value
    dMontoFactura = sivMyApp.Controls("ccurytotinvc_0").value
    
    If Trim(sCustId) <> "" Then
        Sqlst = "Select * from Customer where Custid = " + SParm(sCustId)
        serr1 = SqlFetch1(c1, Sqlst, bCustomer, LenB(bCustomer))
        If serr1 = 0 Then
            If Trim(bCustomer.Terms) = "0" Then
                If dSaldo = 0 Then
                    fnValidaEmbarqueCreditoContado = False
                Else
                    MessBox "Error, cliente CONTADO y embarque con SALDO, favor de capturar los pagos correspondientes", MB_ICONEXCLAMATION, "Error cliente contado"
                    fnValidaEmbarqueCreditoContado = True
                End If
            ElseIf Trim(bCustomer.Terms) <> "0" Then
                If dPagos = 0 Then
                    fnValidaEmbarqueCreditoContado = False
                Else
                MessBox "Error, cliente CRÉDITO y embarque con PAGOS, favor de eliminar los pagos correspondientes y capturar el pago dentro de la pantalla REGISTRAR PAGOS de CUENTAS POR COBRAR", MB_ICONEXCLAMATION, "Error cliente crédito"
                    fnValidaEmbarqueCreditoContado = True
                End If
            End If
        End If
    End If
'---- FIN 001
End Function


